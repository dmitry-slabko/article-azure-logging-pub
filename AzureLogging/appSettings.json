{
  "logConfig": {
    "HttpRequestLogging": true,
    "HttpRequestLogLevel": "Debug",
    "LogToAzureFileSystem": true,
    "AzureFileSizeLimit": 1048576,
    "AzureRollOnSizeLimit": true,
    "AzureRetainedFileCount": 3,
    "AzureRetainTimeLimit": "12:00:00"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "SerilogBootstrap": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}